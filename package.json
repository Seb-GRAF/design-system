  "keywords": [
    "react",
    "mui",
    "design-system"
  ],
  "engines": {
    "node": "20"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
